<?xml version="1.0"?>
<extension key="au.org.sa.remoteattachment" type="module">
  <file>remoteattachment</file>
  <name>remoteattachment</name>
  <description>Upload attachments from remote systems over HTTP</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Angus Fretwell</author>
    <email>angusfretwell@me.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/socialistalternative/civicrm-remote-attachment</url>
    <url desc="Documentation">http://github.com/socialistalternative/civicrm-remote-attachment</url>
    <url desc="Support">http://github.com/socialistalternative/civicrm-remote-attachment</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-05-02</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/RemoteAttachment</namespace>
    <format>23.02.1</format>
    <angularModule>crmRemoteAttachment</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
